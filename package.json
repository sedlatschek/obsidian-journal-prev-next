{
  "name": "obsidian-journal-prev-next",
  "version": "0.0.1",
  "description": "Get a prev and a next link for your daily notes in Obsidian.",
  "license": "ISC",
  "author": "Simon Sedlatschek <code@sedlatschek.com>",
  "type": "commonjs",
  "scripts": {
    "dev": "node esbuild.config.mts",
    "lint": "eslint",
    "svelte-check": "svelte-check --tsconfig tsconfig.json",
    "build": "rimraf --glob dist/* && tsc -noEmit -skipLibCheck && node esbuild.config.mts production",
